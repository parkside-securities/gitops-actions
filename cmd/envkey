#!/usr/bin/env python3
import os
import envkey

env = envkey.fetch_env(os.environ['ENVKEY'], cache_enabled=False)
env_file = os.environ['GITHUB_ENV']

def export_env():
    print(f'::add-mask::{os.environ["ENVKEY"]}') 
    with open(env_file, 'a') as f:
        for k,v in env.items(): 
            f.writeline(f'{k}={v}') 

if __name__ == '__main__':
    export_env()